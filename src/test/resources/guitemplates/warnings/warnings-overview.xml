<TubingGui size="54" id="warnings-overview">
    <title class="gui-title">Warnings</title>

    <GuiItem id="warning-info-0" class="warning-info" slot="0" onLeftClick="manage-warnings/view/detail?warningId=12&amp;backAction=manage-warnings%2Fview%2Foverview" material="PLAYER_HEAD">
        <name class="item-name" color="&amp;3">Warning</name>
        <Lore>
            <LoreLine>
                <t color="&amp;b" id="id-label" class="detail-label">Id: </t>
                <t color="&amp;6" id="id-value" class="detail-value">12</t>
            </LoreLine>

            <LoreLine if="true">
                <t color="&amp;b" id="server-label" class="detail-label">Server: </t>
                <t color="&amp;6" id="server-value" class="detail-value">ServerName</t>
            </LoreLine>

            <LoreLine>
                <t color="&amp;b" id="severity-label" class="detail-label">Severity: </t>
                <t color="&amp;6" id="severity-value" class="detail-value">MINOR</t>
            </LoreLine>

            <LoreLine>
                <t color="&amp;b" id="issuer-label" class="detail-label">Issuer: </t>
                <t color="&amp;6" id="issuer-value" class="detail-value">issuer</t>
            </LoreLine>

            <LoreLine>
                <t color="&amp;b" id="culprit-label" class="detail-label">Culprit: </t>
                <t color="&amp;6" id="culprit-value" class="detail-value">target</t>
            </LoreLine>

            <LoreLine>
                <t color="&amp;b" id="issued-on-label" class="detail-label">Issued on: </t>
                <t color="&amp;6" id="issued-on-value" class="detail-value">01/09/2021-01:11:15</t>
            </LoreLine>

            <LoreLine>
                <t color="&amp;b" id="reason-label" class="detail-label">Reason: </t>
                <t color="&amp;6" id="reason-value" class="detail-value">reason</t>
            </LoreLine>

            <LoreLine></LoreLine>


        </Lore>
    </GuiItem>

    <GuiItem slot="51" id="next-page-0" onLeftClick="goNext" class="next-page" material="GREEN_STAINED_GLASS_PANE">
        <name class="item-name">Next Page</name>
    </GuiItem>
    <GuiItem slot="52" id="next-page-1" onLeftClick="goNext" class="next-page" material="GREEN_STAINED_GLASS_PANE">
        <name class="item-name">Next Page</name>
    </GuiItem>
    <GuiItem slot="53" id="next-page-2" onLeftClick="goNext" class="next-page" material="GREEN_STAINED_GLASS_PANE">
        <name class="item-name">Next Page</name>
    </GuiItem>

</TubingGui>