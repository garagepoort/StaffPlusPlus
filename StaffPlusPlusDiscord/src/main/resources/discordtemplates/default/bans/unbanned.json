// Context:
// ban: IBan => https://github.com/garagepoort/StaffPlusPlus/blob/master/StaffPlusAPI/src/main/java/net/shortninja/staffplus/unordered/IBan.java
// timestamp: String
{
    "content": "Ban update from Staff++",
    "embeds": [
        {
            "title": "User unbanned: ${ban.targetName}",
            "url": "https://www.spigotmc.org/resources/staff.83562/",
            "color": "16620323",
            "timestamp": "${timestamp}",
            "footer": {
                "text": "Provided by Staff++",
                "icon_url": "https://cdn.discordapp.com/embed/avatars/0.png"
            },
            "fields": [
                {
                    "name": "Issuer",
                    "value": "${ban.issuerName}\n[${ban.issuerUuid}]",
                    "inline": true
                },
                {
                    "name": "Banned:",
                    "value": "${ban.targetName}\n[${ban.targetUuid}]",
                    "inline": true
                },
                #IF ban.endDate != null
                {
                    "name": "Type",
                    "value": "TEMPORARY",
                    "inline": false
                },
                {
                    "name": "Ends at: ",
                    "value": "${utils:parseTime(ban.endDate)}",
                    "inline": true
                },
                #ENDIF
                #IF ban.endDate == null
                {
                    "name": "Type",
                    "value": "PERMANENT",
                    "inline": false
                },
                #ENDIF
                {
                    "name": "Ban Reason",
                    "value": "```${ban.reason}```",
                    "inline": false
                },
                {
                    "name": "Unbanned by",
                    "value": "${ban.unbannedByName}\n[${ban.unbannedByUuid}]",
                    "inline": false
                },
                {
                    "name": "Unban Reason",
                    "value": "```${ban.unbanReason}```",
                    "inline": false
                }
            ]
        }
    ]
}