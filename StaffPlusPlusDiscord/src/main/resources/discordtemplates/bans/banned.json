{
    "content": "Ban update from Staff++",
    "embeds": [
        {
            "title": "User banned: ${ban.playerName}",
            "url": "https://www.spigotmc.org/resources/staff.83562/",
            "color": "16601379",
            "timestamp": "${timestamp}",
            "footer": {
                "text": "Provided by Staff++",
                "icon_url": "https://cdn.discordapp.com/embed/avatars/0.png"
            },
            "fields": [
                {
                    "name": "Issuer",
                    "value": "${ban.issuerName}\n[${ban.issuerUuid}]",
                    "inline": true
                },
                {
                    "name": "Banned:",
                    "value": "${ban.playerName}\n[${ban.playerUuid}]",
                    "inline": true
                },
                #IF ban.endDate != null
                {
                    "name": "Type",
                    "value": "TEMPORARY",
                    "inline": false
                },
                {
                    "name": "Ends at: ",
                    "value": "${utils:parseTime(ban.endDate)}",
                    "inline": true
                },
                #ENDIF
                #IF ban.endDate == null
                {
                    "name": "Type",
                    "value": "PERMANENT",
                    "inline": false
                },
                #ENDIF
                {
                    "name": "Ban Reason",
                    "value": "```${ban.reason}```",
                    "inline": false
                }
            ]
        }
    ]
}