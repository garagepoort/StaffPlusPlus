{
    "content": "Mute update from Staff++",
    "embeds": [
        {
            "title": "User muted: ${mute.playerName}",
            "url": "https://www.spigotmc.org/resources/staff.83562/",
            "color": "16601379",
            "timestamp": "${timestamp}",
            "footer": {
                "text": "Provided by Staff++",
                "icon_url": "https://cdn.discordapp.com/embed/avatars/0.png"
            },
            "fields": [
                {
                    "name": "Issuer",
                    "value": "${mute.issuerName + '\\\\n[' + mute.issuerUuid + ']'}",
                    "inline": true
                },
                {
                    "name": "Muted:",
                    "value": "${mute.playerName + '\\\\n[' + mute.playerUuid + ']'}",
                    "inline": true
                },
                #IF mute.endDate != null
                {
                    "name": "Type",
                    "value": "TEMPORARY",
                    "inline": false
                },
                {
                    "name": "Ends at: ",
                    "value": "${utils:parseTime(mute.endDate)}",
                    "inline": true
                },
                #ENDIF
                #IF mute.endDate == null
                {
                    "name": "Type",
                    "value": "PERMANENT",
                    "inline": false
                },
                #ENDIF
                {
                    "name": "Mute Reason",
                    "value": "```${mute.reason}```",
                    "inline": false
                }
            ]
        }
    ]
}