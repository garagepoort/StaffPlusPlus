<TubingGui size="27" id="hub">
    <title class="gui-title">&amp;eGUI Hub</title>
    <GuiItem slot="0" id="color-select-0" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="8" id="color-select-1" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="9" id="color-select-2" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="17" id="color-select-3" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="18" id="color-select-4" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="26" id="color-select-5" material="STAINED_GLASS_PANE" onLeftClick="$NOOP">
        <name class="item-name" color="&amp;b">Color #STAINED_GLASS_PANE</name>
    </GuiItem>
    <GuiItem slot="1" id="open-reports" material="PAPER" onLeftClick="manage-reports/view/open?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bUnresolved reports</name>
        <Lore>
            <LoreLine>
                <t id="reports-lore" color="&amp;7">&amp;7Shows all open reports.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="my-reports" slot="2" material="PAPER" onLeftClick="manage-reports/view/my-assigned?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bReports assigned to you</name>
        <Lore>
            <LoreLine>
                <t id="my-reports-lore" color="&amp;7">&amp;7Shows reports currently assigned to you.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="assigned-reports" slot="3" material="PAPER" onLeftClick="manage-reports/view/assigned?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bReports In Progress</name>
        <Lore>
            <LoreLine>
                <t id="assigned-reports-lore" color="&amp;7">&amp;7Shows reports that are currently being worked on.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="closed-reports" slot="4" material="PAPER" onLeftClick="manage-reports/view/closed?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bClosed reports</name>
        <Lore>
            <LoreLine>
                <t id="closed-reports-lore" color="&amp;7">&amp;7History of all closed reports.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="miner" slot="10" if="true" material="STONE_PICKAXE" onLeftClick="miners/view?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bMiner GUI</name>
        <Lore>
            <LoreLine>
                <t id="miner-lore" color="&amp;7">&amp;7Shows all players under the set Y value.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="protected-areas" slot="19" if="true" material="SHIELD" onLeftClick="protected-areas/view?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bProtected Areas GUI</name>
        <Lore>
            <LoreLine>
                <t id="protected-areas-lore" color="&amp;7">&amp;7Shows all currently set protected areas.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="bans" slot="7" if="true" material="BANNER" onLeftClick="manage-bans/view/overview?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bBanned players GUI</name>
        <Lore>
            <LoreLine>
                <t id="bans-lore" color="&amp;7">&amp;7Shows all currently banned players.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="mutes" slot="16" if="true" material="SIGN" onLeftClick="manage-mutes/view/all-active?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bMuted players GUI</name>
        <Lore>
            <LoreLine>
                <t id="mutes-lore" color="&amp;7">&amp;7Shows all currently muted players.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
    <GuiItem id="investigations" slot="25" if="true" material="BOOK" onLeftClick="manage-investigations/view/overview?backAction=hub%2Fview">
        <name class="item-name" color="&amp;b">&amp;bInvestigations GUI</name>
        <Lore>
            <LoreLine>
                <t id="investigations-lore" color="&amp;7">&amp;7Shows all investigations.</t>
            </LoreLine>
        </Lore>
    </GuiItem>
</TubingGui>